diff -Naur fontconfig-2.10.2.orig/etc/fonts/infinality/conf.d/20-fix-cantarell.conf fontconfig-2.10.2/etc/fonts/infinality/conf.d/20-fix-cantarell.conf
--- fontconfig-2.10.2.orig/etc/fonts/infinality/conf.d/20-fix-cantarell.conf	1970-01-01 01:00:00.000000000 +0100
+++ fontconfig-2.10.2/etc/fonts/infinality/conf.d/20-fix-cantarell.conf	2013-01-05 02:11:58.000000000 +0100
@@ -0,0 +1,17 @@
+<?xml version='1.0'?>
+<!DOCTYPE fontconfig SYSTEM 'fonts.dtd'>
+<fontconfig>
+
+	<!-- ##Style: common -->
+
+	<!-- Fixes issues where Cantarell is not being displayed -->
+	<match target="pattern">
+		<test name="family">
+			<string>Cantarell</string>
+		</test>
+		<edit name="family" mode="assign" binding="strong">
+			<string>Cantarell</string>
+		</edit>
+	</match>
+
+</fontconfig>
